        <h1>Catégorie : {{category.name}}</h1>
        <div class="row">
            {% for article in category.articles %}
                <div class="col-md-4">
                    {% include "partials/_article.html.twig" %}
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
